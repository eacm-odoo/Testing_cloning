        <record id="stock_quant_filter" model="ir.filters">
            <field name="name">Stock quants</field>
            <field name="model_id">stock.quant</field>
            <!-- <field name="user_id" eval="False"/>
            <field name="domain">[('project_id', '!=', False)]</field>
            <field name="is_default" eval="True"/>
            <field name="context">{
                'col_group_by': ['date:month', 'timesheet_invoice_type'],
                'group_by': ['project_id', 'employee_id'],
                'measures': ['amount_currency', 'unit_amount', '__count']
            }</field>
            <field name="action_id" ref="timesheet_action_billing_report"/> -->
        </record>

        <record id="edi_stock_connection" model="edi.connection">
            <field name="name">EDI: stock</field>
            <field name="type">sftp</field>
            <field name="configuration"><![CDATA[
{
    "host": "test.rebex.net",
    "user": "demo",
    "password": "password",
    "in_folder": "/",
    "in_folder_done": "/",
    "in_folder_error": "/",
    "on_conflict": "replace"
}
            ]]>            </field>
